          "etape_21": "Traçabilité finale workflow libre pratique"
        },
        "transit": {
          "etapes_totales": 16,
          "etapes_commission": "16",
          "etape_16": "Traçabilité finale transit"
        }
      },
      "pays_surveilles": {
        "cotiers": ["SEN", "CIV", "BEN", "TGO", "GNB"],
        "hinterland": ["MLI", "BFA", "NER"],
        "total": 8
      },
      "kit_mulesoft": {
        "url": "https://kit-interconnexion-uemoa-v4320.m3jzw3-1.deu-c1.cloudhub.io",
